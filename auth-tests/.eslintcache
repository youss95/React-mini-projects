rrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/sayongkim/Documents/react-projects/auth-tests/src/index.js",[],["56","57"],"/Users/sayongkim/Documents/react-projects/auth-tests/src/App.js",["58","59","60"],"/Users/sayongkim/Documents/react-projects/auth-tests/src/pages/AuthPage.js",[],"/Users/sayongkim/Documents/react-projects/auth-tests/src/pages/HomePage.js",[],"/Users/sayongkim/Documents/react-projects/auth-tests/src/components/Profile/UserProfile.js",[],"/Users/sayongkim/Documents/react-projects/auth-tests/src/components/Layout/Layout.js",[],"/Users/sayongkim/Documents/react-projects/auth-tests/src/components/Profile/ProfileForm.js",[],"/Users/sayongkim/Documents/react-projects/auth-tests/src/components/StartingPage/StartingPageContent.js",[],"/Users/sayongkim/Documents/react-projects/auth-tests/src/components/Auth/AuthForm.js",["61"],"import { useContext, useEffect, useRef, useState } from \"react\";\nimport axios from \"axios\";\nimport classes from \"./AuthForm.module.css\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { loginTest } from \"../../moduels/auth\";\nconst AuthForm = () => {\n  const dispatch = useDispatch();\n  const tokenId = useSelector((state) => state.auth.token);\n  const isLoggedIn = useSelector((state) => state.auth.isLoggedIn);\n  const [isLogin, setIsLogin] = useState(false);\n  const [isLoading, setIsLoading] = useState(false);\n\n  const emailInputRef = useRef();\n  const passwordInputRef = useRef();\n  const switchAuthModeHandler = () => {\n    setIsLogin((prevState) => !prevState);\n  };\n  useEffect(() => {\n    console.log(\"s\", tokenId);\n    console.log(\"d\", isLoggedIn);\n  });\n  const submitHandler = async (event) => {\n    event.preventDefault();\n    const email = emailInputRef.current.value;\n    const password = passwordInputRef.current.value;\n    setIsLoading(true);\n    let url;\n    if (isLogin) {\n      url =\n        \"https://identitytoolkit.googleapis.com/v1/accounts:signInWithPassword?key=AIzaSyD_QepXNq_Fxe5EkmUQroULB6m269QxBZg\";\n    } else {\n      url =\n        \"https://identitytoolkit.googleapis.com/v1/accounts:signUp?key=AIzaSyD_QepXNq_Fxe5EkmUQroULB6m269QxBZg\";\n    }\n    try {\n      const authSt = await axios.post(url, {\n        email: email,\n        password: password,\n        returnSecureToken: true,\n      });\n      console.log(\"sss\", authSt);\n      let token = authSt.data.idToken;\n      dispatch(loginTest(token));\n    } catch (e) {\n      let errMsg = e.response.data.error.message;\n      //console.log(errMsg);\n      alert(errMsg);\n    } finally {\n      setIsLoading(false);\n    }\n  };\n\n  return (\n    <section className={classes.auth}>\n      <h1>{isLogin ? \"Login\" : \"Sign Up\"}</h1>\n      <form onSubmit={submitHandler}>\n        <div className={classes.control}>\n          <label htmlFor=\"email\">Your Email</label>\n          <input type=\"email\" id=\"email\" required ref={emailInputRef} />\n        </div>\n        <div className={classes.control}>\n          <label htmlFor=\"password\">Your Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            required\n            ref={passwordInputRef}\n          />\n        </div>\n        <div className={classes.actions}>\n          {!isLoading && (\n            <button>{isLogin ? \"Login\" : \"Create Account\"}</button>\n          )}\n          {isLoading && <p style={{ color: \"white\" }}>Loading...</p>}\n          <button\n            type=\"button\"\n            className={classes.toggle}\n            onClick={switchAuthModeHandler}\n          >\n            {isLogin ? \"Create new account\" : \"Login with existing account\"}\n          </button>\n        </div>\n      </form>\n    </section>\n  );\n};\n\nexport default AuthForm;\n","/Users/sayongkim/Documents/react-projects/auth-tests/src/components/Layout/MainNavigation.js",[],"/Users/sayongkim/Documents/react-projects/auth-test